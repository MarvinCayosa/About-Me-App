<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OtherPage"
    android:background="@color/white">
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginEnd="20dp"
    android:layout_marginStart="20dp">
    <ImageView
        android:id="@+id/img_other_header"
        android:layout_width="250dp"
        android:layout_height="125dp"
        android:layout_marginTop="40dp"
        android:layout_alignParentStart="true"
        android:src="@drawable/text_end"/>
    <ImageButton
        android:id="@+id/fb"
        android:layout_width="165dp"
        android:layout_height="280dp"
        android:layout_marginTop="10dp"
        android:layout_below="@id/img_other_header"
        android:adjustViewBounds="true"
        android:background="@color/white"
        android:scaleType="centerInside"
        android:src="@drawable/fb"/>
    <ImageButton
        android:id="@+id/ig"
        android:layout_width="190dp"
        android:layout_height="280dp"
        android:layout_marginTop="10dp"
        android:layout_marginStart="15dp"
        android:layout_below="@id/img_other_header"
        android:adjustViewBounds="true"
        android:layout_toEndOf="@id/fb"
        android:background="@color/white"
        android:scaleType="centerInside"
        android:src="@drawable/insta"/>

    <ImageButton
        android:id="@+id/tk"
        android:layout_width="match_parent"
        android:layout_height="175dp"
        android:layout_below="@id/fb"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="-16dp"
        android:layout_marginEnd="-1dp"
        android:adjustViewBounds="true"
        android:background="@color/white"
        android:scaleType="centerInside"
        android:src="@drawable/tiktok" />

    <ImageView
        android:id="@+id/img_tag"
        android:layout_width="166dp"
        android:layout_height="47dp"
        android:layout_below="@id/tk"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="10dp"
        android:src="@drawable/tag" />

</RelativeLayout>
</RelativeLayout>